        <div class="product-page__info">
            <div class="product-page__status product-order"> {% if quantity > 0 %} {{ stock }} {% else %} {{ text_preorder_no_price }} {% endif %}</div>
            <div class="product-page__article">{{ text_model }} <span>{{ model }}</span></div>
        </div>
        {% if options %}
            {% for option in options %}
                <div class="product-page__color">
                    <select class="select-color" name="option[{{ option.product_option_id }}]" id="input-option{{ option.product_option_id }}">
                        <option value="" selected disabled>{{ text_select }}</option>
                        {% for option_value in option.product_option_value %}
                            <option value="{{ option_value.product_option_value_id }}">{{ option_value.name }}{% if option_value.price %} ({{ option_value.price_prefix }}{{ option_value.price }}){% endif %}</option>
                        {% endfor %}
                    </select>
                    <div class="select-label">{{ option.name }}:</div>
                </div>
            {% endfor %}
        {% endif %}

