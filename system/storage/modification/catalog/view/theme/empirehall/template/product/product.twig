{{ header }}
<link href="https://empirehall.com.ua/catalog/view/theme/default/stylesheet/index.css" rel="stylesheet"> 
<div class="head-block">
    <ol class="breadcrumb" itemscope itemtype="http://schema.org/BreadcrumbList">
        {% for counter, breadcrumb in breadcrumbs %}
            {% if counter + 1 < breadcrumbs|length %}
                <li itemprop="itemListElement" itemscope itemtype="http://schema.org/ListItem">
                    <a itemprop="item" href="{{ breadcrumb.href }}"><span itemprop="name">{{ breadcrumb.text }}</span></a>
                    <meta itemprop="position" content="{{ counter+1 }}" />
                </li>
            {% else %}
                <li itemprop="itemListElement" itemscope itemtype="http://schema.org/ListItem">
                    <span itemprop="name">{{ breadcrumb.text }}</span>
                    <meta itemprop="position" content="{{ counter+1 }}" />
                </li>
            {% endif %}
        {% endfor %}
    </ol>
</div>
<div class="product-page">
    {% if images %}
        <div class="product-page__left">
            <div class="product-page__slider-wrap">
                <div class="product-page__lg-slider">
                    {% for image in images %}
                        <a href="{{ image.popup_big }}" data-fancybox="product-page" class="product-page__lg-slide">
                            <img decoding="async" src="data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAAAEAAAABCAYAAAAfFcSJAAAAAXNSR0IArs4c6QAAAARnQU1BAACxjwv8YQUAAAAJcEhZcwAADsQAAA7EAZUrDhsAAAANSURBVBhXYzh8+PB/AAffA0nNPuCLAAAAAElFTkSuQmCC" data-src="{{ image.popup }}" width="620" height="620" alt="{{ heading_title }} + {{ price }} Empire Hall"  class="lazyload">
                        </a>
                    {% endfor %}
                </div>
                {% if images|length > 1 %}
                <div class="product-page__sm-slider">
                    {% for image in images %}
                        <div class="product-page__sm-slide">
                            <img decoding="async" src="data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAAAEAAAABCAYAAAAfFcSJAAAAAXNSR0IArs4c6QAAAARnQU1BAACxjwv8YQUAAAAJcEhZcwAADsQAAA7EAZUrDhsAAAANSURBVBhXYzh8+PB/AAffA0nNPuCLAAAAAElFTkSuQmCC"  data-src="{{ image.thumb }}" width="80" height="80" alt="{{ heading_title }} + {{ price }} Empire Hall"  class="lazyload">
                        </div>
                    {% endfor %}
                </div>
                {% endif %}
                <div class="slider-buttons">
                    <div class="slider-buttons__button zoom">
                        <svg><use xmlns:xlink="https://www.w3.org/1999/xlink" xlink:href="front/dist/images/sprite.svg#fullpage"></use></svg>
                    </div>
                    {% if video %}
                        <div class="slider-buttons__button video popup-open" data-for="popup-video">
                            <svg><use xmlns:xlink="https://www.w3.org/1999/xlink" xlink:href="front/dist/images/sprite.svg#play"></use></svg>
                        </div>
                    {% endif %}
                </div>
            </div>
        </div>
    {% else %}
        <div class="product-page__left">
            <div class="product-page__slider-wrap">
                <div class="product-page__lg-slider">
                    <a href="{{ no_image }}" data-fancybox="product-page" class="product-page__lg-slide">
                        <img decoding="async" src="data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAAAEAAAABCAYAAAAfFcSJAAAAAXNSR0IArs4c6QAAAARnQU1BAACxjwv8YQUAAAAJcEhZcwAADsQAAA7EAZUrDhsAAAANSURBVBhXYzh8+PB/AAffA0nNPuCLAAAAAElFTkSuQmCC" data-src="{{ no_image }}" width="620" height="620" alt="{{ heading_title }} + {{ price }} Empire Hall" class="lazyload">
                        {# alt="{{ heading_title }} + {{ price }} Empire Hall" Пункт в ТЗ 33 #}
                    </a>
                </div>
                <div style="display: none;" class="product-page__sm-slider">
                    <div class="product-page__sm-slide">
                        <img decoding="async" src="data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAAAEAAAABCAYAAAAfFcSJAAAAAXNSR0IArs4c6QAAAARnQU1BAACxjwv8YQUAAAAJcEhZcwAADsQAAA7EAZUrDhsAAAANSURBVBhXYzh8+PB/AAffA0nNPuCLAAAAAElFTkSuQmCC" data-src="{{ no_image_thumb }}" width="80" height="80" alt="" class="lazyload">
                    </div>
                </div>
                <div class="slider-buttons">
                    <div class="slider-buttons__button zoom">
                        <svg><use xmlns:xlink="https://www.w3.org/1999/xlink" xlink:href="front/dist/images/sprite.svg#fullpage"></use></svg>
                    </div>
                    {% if video %}
                        <div class="slider-buttons__button video popup-open" data-for="popup-video">
                            <svg><use xmlns:xlink="https://www.w3.org/1999/xlink" xlink:href="front/dist/images/sprite.svg#play"></use></svg>
                        </div>
                    {% endif %}
                </div>
            </div>
        </div>
    {% endif %}
